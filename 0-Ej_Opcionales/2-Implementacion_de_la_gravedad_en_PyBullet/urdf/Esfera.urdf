<?xml version="1.0" ?>
<robot name="Gravity_Sphere">
    <link name="Sphere">
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <mass value="10.0"/>
            <inertia ixx="1.0" ixy="0.0" ixz="0.0"
                               iyy="1.0" iyz="0.0"
                                         izz="1.0"/>
        </inertial>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
				<sphere radius="0.2"/>
            </geometry>
            <material name="white">
                <color rgba="1 1 1 1"/>
            </material>
        </visual>

        <collision>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
	            <sphere radius="0.2"/>
            </geometry>
        </collision>

    </link>
</robot>